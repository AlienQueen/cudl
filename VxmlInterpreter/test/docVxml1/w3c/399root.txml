<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance">
  <script><![CDATA[ 
    function myFactorial(n) 
         {
             return (n <= 1)? 1 : n * myFactorial(n-1);
    } 
        ]]></script>
  <var name="applVar1" expr="myFactorial(3)"/>
</vxml>
