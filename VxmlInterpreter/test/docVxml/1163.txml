<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance">
  <catch>
    <conf:fail expr="'document caught event ' + _event"/>
  </catch>
  <var name="varUsedInLog" expr="'labelThatMustBePresentInLogMessage'"/>
  <form>
    <block>
      <log expr="varUsedInLog">This is a log message.</log>
      Look into logging message to verify that the log message is reported.
      <conf:pass/>
    </block>
  </form>
</vxml>
