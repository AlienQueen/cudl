<vxml version="2.0" 
   xmlns="http://www.w3.org/2001/vxml"
   xmlns:conf="http://www.w3.org/2002/vxml-conformance"
>

<!-- Assertion 88: 
  A subdialog's new context may itself invoke a subdialog. -->

  <catch>
    <conf:fail expr="'unexpected error: '+ _event" />
  </catch>

  <form>
    <block>
      <conf:comment>
        Attempting to go to a88-1.txml that executes a subdialog.
      </conf:comment>
      <goto next="a88-1.txml" />
    </block>
  </form>
</vxml>