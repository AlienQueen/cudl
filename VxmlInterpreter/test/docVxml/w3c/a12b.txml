<vxml version="2.0" 
  xmlns="http://www.w3.org/2001/vxml" 
  xmlns:conf="http://www.w3.org/2002/vxml-conformance">

  <catch><conf:fail expr="'catch-all caught ' + _event"/></catch>
  <form id="firstForm">
    <block>
      <conf:pass/>
    </block>
  </form>
  
  <form id="secondForm">
    <block>
      <conf:fail reason="second form executed with no fragment"/>
    </block>
  </form>
</vxml>
