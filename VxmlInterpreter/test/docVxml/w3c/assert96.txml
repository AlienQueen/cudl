<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml"
      xmlns:conf="http://www.w3.org/2002/vxml-conformance">

<!--
    A form may contain <var> elements.
-->

  <catch>
    <conf:comment>
      Toplevel caught event <value expr="_event"/>
    </conf:comment>
    <conf:fail expr="_event"/>
  </catch>


  <form id="first">
    <var name="a" expr="'AAA'"/>
    <block>
      <conf:pass/>
    </block>
  </form>

</vxml>
